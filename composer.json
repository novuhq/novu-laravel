{
    "name": "novu/novu-laravel",
    "description": "Novu for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Novu contributors",
            "email": "prosperotemuyiwa@gmail.com",
            "homepage": "https://github.com/novuhq/novu-laravel"
        }
    ],
    "homepage": "https://novu.co",
    "keywords": [
        "Laravel",
        "novu",
        "novu-laravel",
        "sdk",
        "notifications",
        "php",
        "laravel 8",
        "laravel 9",
        "laravel 10"
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "illuminate/support": "~6|~7|~8|^9.0|^10.0",
        "unicodeveloper/novu": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~4|~5|~6|^7.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Novu\\Laravel\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Novu\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novu\\Laravel\\NovuServiceProvider"
            ],
            "aliases": {
                "Novu": "Novu\\Laravel\\Facades\\Novu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
